{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "Includi-Environment3D/Submodules/Kit-Shared/Assembly-CSharp.csproj"
          ],
          "exclude": ["**/bin/**", "**/obj/**", "docs/**"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": [
          "userguide/**.md",
          "userguide/**/toc.yml",
          "developerguide/**.md",
          "developerguide/**/toc.yml",
          "activities/**.md",
          "activities/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "docs/**"]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
