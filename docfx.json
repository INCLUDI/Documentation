{
  "build": {
    "content": [
      {
        "files": [
          "guide-kit/**.md",
          "guide-kit/**/toc.yml",
          "guide-webapp/**.md",
          "guide-webapp/**/toc.yml",
          "activities/**.md",
          "activities/**/toc.yml",
          "tech-docs/**.md",
          "tech-docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "guide-kit/images/**",
          "guide-webapp/images/**",
          "activities/images/**",
          "tech-docs/images/**",
          "favicon.ico"
        ]
      }
    ],
    "globalMetadata": {
      "_appFaviconPath": "favicon.ico"
    },
    "overwrite": [
      {
        "exclude": ["obj/**", "docs/**"]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}
