{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "Includi-Environment3D/Submodules/Kit-Shared/Assembly-CSharp.csproj"
          ],
          "exclude": ["**/bin/**", "**/obj/**", "docs/**"]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": [
          "guide-kit/**.md",
          "guide-kit/**/toc.yml",
          "guide-webapp/**.md",
          "guide-webapp/**/toc.yml",
          "activities/**.md",
          "activities/**/toc.yml",
          "tech-docs/**.md",
          "tech-docs/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "guide-kit/images/**",
          "guide-webapp/images/**",
          "activities/images/**",
          "tech-docs/images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appFaviconPath": "favicon.ico"
    },
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "docs/**"]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}
